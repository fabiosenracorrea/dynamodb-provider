{
  "homepage": "https://github.com/fabiosenracorrea/dynamodb-provider",
  "author": "fabiosenracorrea",
  "name": "dynamodb-provider",
  "version": "3.1.3",
  "description": "A dynamodb Provider that simplifies the native api. It packs with a Single Table adaptor/pseudo ORM for single table design",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "bugs": {
    "url": "https://github.com/fabiosenracorrea/dynamodb-provider/issues"
  },
  "scripts": {
    "clean:maps": "find lib -name '*.map' -delete",
    "build-both": "rm -rf ./lib yarn check-ts && yarn build:esm && yarn build:cjs && yarn clean:maps",
    "build:esm": "yarn check-ts && tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:cjs": "yarn check-ts && tsc --project tsconfig.cjs.json --outDir lib/cjs && tsc-alias -p tsconfig.cjs.json",
    "build": "tsc --noEmit",
    "check-ts": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "watch": "tsc-watch --onSuccess \"node dist/index.js\"",
    "run-local-file": "npx ts-node -r tsconfig-paths/register -P ./tsconfig.json src/index.ts",
    "build-publish": "yarn build-both && npm publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "aws-sdk": "^2.1048.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.1",
    "vitepress": "^1.6.4"
  },
  "dependencies": {
    "ksuid": "^3.0.0"
  },
  "files": [
    "/lib",
    "README.md",
    "changelog.md",
    "LICENSE"
  ],
  "keywords": [
    "dynamodb",
    "aws"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiosenracorrea/dynamodb-provider"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
